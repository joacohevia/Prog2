package avion;

public class main {

	public static void main(String[] args) {
		Avion1 p1111 = new Avion1();
		Avion1 p22 = new Avion1(25,"marron");
		Avion1 p33 = new Avion1(100,"rojo","ford");
		Avion1 p44 = new Avion1 ("chevrolet",120,"viole");
		
		System.out.println(p1111.getCantidad());
		System.out.println("----");
		System.out.println(p22.getCantidad());
		System.out.println(p22.getColor());
		System.out.println(p22.getMarca());
		System.out.println("----");
		System.out.println(p33.getCantidad());
		System.out.println(p33.getColor());
		System.out.println(p33.getMarca());
		System.out.println("----");
		System.out.println(p44.getCantidad());
		System.out.println(p44.getColor());
		System.out.println(p44.getMarca());


		

	}

}
